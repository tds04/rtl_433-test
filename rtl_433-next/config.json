    "mqtt_retain": "bool?",
    "mqtt_topic": "str?",
    "report_meta": "bool?",
    "report_protocol": "bool?",
    "log_level": "int(1,8)?",
    "log_protocol": "bool?",
    "log_meta": "bool?",
    "pulse_detect_auto": "bool?",
    "output_json": "bool?",
    "output_mqtt": "bool?",
    "output_kv": "bool?"
  }
